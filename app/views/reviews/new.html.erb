<div class = 'container'>
  <h2>Thanks for reviewing <%= @restaurant.name%>!</h2>
    <br>
    <%= simple_form_for [@restaurant, @review] do |f| %>
      <%= f.input :rating, collection: %w{0 1 2 3 4 5}  %>
      <%= f.input :content %>

      <%= f.submit class: 'btn btn-primary' %>
    <% end %>
</div>
